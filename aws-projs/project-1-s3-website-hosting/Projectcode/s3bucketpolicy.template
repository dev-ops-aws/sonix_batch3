AWSTemplateFormatVersion: '2010-09-09'
Resources:
  S3BucketPolicy:
    Type: 'AWS::S3::BucketPolicy'
    Properties:
      Bucket: prasanthkumar.000
      PolicyDocument:
        Statement:
          - Sid: PublicReadGetObject
            Effect: Allow
            Principal: '*'
            Action: 's3:GetObject'
            Resource: 
              Fn::Sub:
                - arn:aws:s3:::prasanthkumar.000/*
                - {}
